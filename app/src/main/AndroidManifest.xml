<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Tarea_CRUD_SQlite_MVC">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TareaMVC">

        <activity
            android:name="com.example.Tarea_CRUD_SQlite_MVC.NewActivity"
            android:parentActivityName="com.example.Tarea_CRUD_SQlite_MVC.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Tarea_CRUD_SQlite_MVC.MainActivity" />
        </activity>

        <activity android:name="com.example.Tarea_CRUD_SQlite_MVC.ViewActicity">
            android:name=".ViewActivity"
            android:parentActivityName=".MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Tarea_CRUD_SQlite_MVC.MainActivity" />
        </activity>

        <activity
            android:name="com.example.Tarea_CRUD_SQlite_MVC.UpdateActivity"
            android:parentActivityName="com.example.Tarea_CRUD_SQlite_MVC.MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Tarea_CRUD_SQlite_MVC.MainActivity" />
        </activity>
        <activity android:name="com.example.Tarea_CRUD_SQlite_MVC.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>